{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/amineabri/base-repository.git"
    }
  ],
  "name": "lara-lab/oauth-token-validator",
  "description": "The package adds some useful functionality to a Laravel/Lumen app which wants to be an oAuth2 client.",
  "type": "library",
  "require": {
    "php": "^7.3",
    "codercat/jwk-to-pem": "^0.0.3",
    "guzzlehttp/guzzle": "^6.3",
    "lcobucci/jwt": "^3.2",
    "illuminate/support": "^5.8",
    "illuminate/database": "^5.8",
    "illuminate/routing": "^5.8",
    "illuminate/contracts": "^5.8",
    "illuminate/cache": "^5.8",
    "illuminate/config": "^5.8",
    "illuminate/auth": "^5.8",
    "illuminate/hashing": "^5.8",
    "amineabri/base-repository": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^8.2",
    "zircote/swagger-php": "^3.0",
    "squizlabs/php_codesniffer": "^3.5",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpstan/phpstan": "^0.11.19",
    "sensiolabs/security-checker": "^6.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Amine Abri",
      "email": "amineabri@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "AB\\OAuthTokenValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AB\\OAuthTokenValidator\\Tests\\": "tests/"
    }
  }
}
