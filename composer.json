{
  "name": "lara-lab/oauth-token-validator",
  "description": "The package adds some useful functionality to a Laravel/Lumen app which wants to be an oAuth2 client.",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/amineabri/base-repository.git"
    }
  ],
  "require": {
    "php": "^7.3",
    "codercat/jwk-to-pem": "^0.0.3",
    "guzzlehttp/guzzle": "^7.1",
    "lcobucci/jwt": "^3.3",
    "illuminate/support": "^8.8",
    "illuminate/database": "^8.8",
    "illuminate/routing": "^8.8",
    "illuminate/contracts": "^8.8",
    "illuminate/cache": "^8.8",
    "illuminate/config": "^8.8",
    "illuminate/auth": "^8.8",
    "illuminate/hashing": "^8.8",
    "amineabri/base-repository": "^1.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "phpunit/phpunit": "^9.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Amine Abri",
      "email": "amineabri@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "AB\\OAuthTokenValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AB\\OAuthTokenValidator\\Tests\\": "tests/"
    }
  }
}
